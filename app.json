{
  "expo": {
    "name": "bhakti",
    "slug": "bhakti",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/logo/logo.jpeg",
    "scheme": "bhakti",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/logo/logo.jpeg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.manish774.bhakti"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "../assets/logo/logo.jpe"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b094017f-48d6-454b-871e-65ca4af87cb8"
      }
    }
  }
}
